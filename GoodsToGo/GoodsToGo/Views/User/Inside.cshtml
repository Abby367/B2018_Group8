<!--start wrapper-->
<div class="wrapper">

    <!--start sidebar -->
    <aside class="sidebar-wrapper" data-simplebar="true">
        <div class="sidebar-header">
            <div>

                <img src="~/assets/images/logo-icon-2.png" class="logo-icon" alt="logo icon" />
            </div>
            <div>

            </div>
            <div class="toggle-icon ms-auto">
                <ion-icon name="menu-sharp"></ion-icon>
            </div>
        </div>
        <!--navigation-->
        <ul class="metismenu" id="menu">
            <li>
                <a href=@Url.Action("Inside", "User")>
                    <div class="parent-icon">
                        <ion-icon name="home-sharp"></ion-icon>
                    </div>
                    <div class="menu-title">Home</div>
                </a>
            </li>
            <li>
                <a href=@Url.Action("Details", "User", new {id = "0"})>
                    <div class="parent-icon">
                        <ion-icon name="person-circle-sharp"></ion-icon>
                    </div>
                    <div class="menu-title">User Profile</div>
                </a>
            </li>
            <li>

                <a href=@Url.Action("Edit", "User", new {id =0 })>
                    <div class="parent-icon">
                        <ion-icon name="create-sharp"></ion-icon>
                    </div>
                    <div class="menu-title">Edit Profile</div>
                </a>
            </li>


            <li>
                <a href="pages-invoices.html">
                    <div class="parent-icon">
                        <ion-icon name="receipt-sharp"></ion-icon>
                    </div>
                    <div class="menu-title">Privacy And Policy</div>
                </a>
            </li>
            <li>
                <a href="pages-to-do.html">
                    <div class="parent-icon">
                        <ion-icon name="shield-checkmark-sharp"></ion-icon>
                    </div>
                    <div class="menu-title">Terms And Condition </div>
                </a>

            </li>

        </ul>
        <!--end navigation-->
    </aside>
    <!--end sidebar -->
    <!--start top header-->
    <header class="top-header">
        <nav class="navbar navbar-expand gap-3">
            <div class="mobile-menu-button">
                <ion-icon name="menu-sharp"></ion-icon>
            </div>

            <div class="top-navbar-right ms-auto">


                <ul class="navbar-nav align-items-center">
                    <li>
                        <a href="index.html" class="logo-text">Goods To Go</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link dark-mode-icon" href="javascript:;">
                            <div class="mode-icon">
                                <ion-icon name="moon-sharp"></ion-icon>
                            </div>
                        </a>
                    </li>
                </ul>



            </div>
        </nav>
    </header>
    <!--end top header-->
    <!-- start page content wrapper-->
    <div class="page-content-wrapper">
        <!-- start page content-->
        <div class="page-content">

            <!--start breadcrumb-->
            <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
                <div class="breadcrumb-title pe-3">Dashboard</div>
                <div class="ps-3">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb mb-0 p-0 align-items-center">
                            <li class="breadcrumb-item">
                                <a href="javascript:;">
                                    <ion-icon name="home-outline"></ion-icon>
                                </a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page">Create Profile</li>
                        </ol>
                    </nav>

                </div>
                <div class="ms-auto">
                    <div class="btn-group">

                        @Html.ActionLink("Logout", "Logout", "User", new { @type = "button", @class = "btn btn-outline-primary" })

                    </div>
                </div>
            </div>
            <!--end breadcrumb-->

            <div class="row row-cols-1 row-cols-lg-3">
                <div class="col">
                    <div class="card radius-10">
                        <div class="card-body">
                            <div class="d-flex align-items-center gap-2">
                                <div class="fs-5">
                                    <ion-icon name="person-add-outline"></ion-icon>
                                </div>
                                <div>
                                    <a href=@Url.Action("CreateProfile", "User")>
                                        Create

                                        Profile
                                    </a>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
                <div class="col">
                    <div class="card radius-10">
                        <div class="card-body">
                            <div class="d-flex align-items-center gap-2">
                                <div class="fs-5">
                                    <ion-icon name="heart-outline"></ion-icon>
                                </div>
                                <div>
                                    <a href=@Url.Action("Book", "User") class="mb-0">
                                        Book

                                        Schedule
                                    </a>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
                <div class="col">
                    <div class="card bg-gradient-purple radius-10">

                        <div class="card-body">
                            <div class="d-flex align-items-center gap-2">
                                <div class="fs-5">
                                    <ion-icon name="chatbox-outline"></ion-icon>
                                </div>
                                <div>
                                    <a href=@Url.Action("History", "User") class="mb-0">
                                        History
                                    </a>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>

            </div>
            <!--end row-->
            <!--end row-->
        </div>
    </div>
